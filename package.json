  "main": "src/index.js",
  "keywords": [
    "slack",
    "release",
    "note",
    "notes"
  ],
  "devDependencies": {
    "@invisible/eslint-config": "^1.0.0",
    "chance": "^1.0.6",
    "docdash": "^0.4.0",
    "eslint": "^4.7.2",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0",
    "mock-require": "^2.0.1",
    "nsp": "^2.6.3",
    "nyc": "^10.3.0",
    "recursive-readdir": "^2.1.1",
    "sinon": "^2.2.0",
    "sinon-express-mock": "^1.2.4"
  },
  "repository": "git@github.com:invisible-tech/release-note.git",
  "scripts": {
    "builddoc": "./node_modules/.bin/jsdoc -c ./.jsdoc.json",
